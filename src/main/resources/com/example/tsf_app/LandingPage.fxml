        <Button fx:id="btnSearch_Bar" alignment="CENTER" contentDisplay="CENTER" layoutX="266.0" layoutY="19.0" minWidth="53.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="63.0" style="-fx-background-color: transparent; -fx-background-radius: 35;" text="SEARCH" textFill="#7d7777" />
