<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="335.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tsf_app.tsf_Login_Page_Controller">
   <children>
      <ImageView fx:id="img_User_Profile" fitHeight="110.0" fitWidth="110.0" layoutX="108.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btnEdit_profile" layoutX="43.0" layoutY="288.0" mnemonicParsing="false" onAction="#edit_Profile" prefWidth="120.0" style="-fx-background-radius: 15;" text="EDIT PROFILE">
         <font>
            <Font name="System Bold" size="12.0" />
      </font>
      </Button>
      <Button fx:id="btnDelete_Account" layoutX="173.0" layoutY="288.0" mnemonicParsing="false" prefWidth="120.0" style="-fx-background-radius: 15;" text="DELETE ACCOUNT">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="lbl_Full_Name" layoutX="120.0" layoutY="191.0" text="FULL NAMES">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="lbl_Status_Msg" layoutX="114.0" layoutY="228.0" text="STATUS MESSAGE" textFill="#1029cc">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Button fx:id="btn_Pro_Pic" layoutX="190.0" layoutY="134.0" mnemonicParsing="false" onAction="#choosePro_Pic" prefHeight="25.0" prefWidth="25.0" style="-fx-background-radius: 50;">
         <graphic>
            <ImageView fitHeight="15.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@cam.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="btnOnProfile_Back" layoutX="33.0" layoutY="25.0" maxWidth="35.0" mnemonicParsing="false" onAction="#goBack" prefHeight="20.0" style="-fx-background-color: white;">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@back_Icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
